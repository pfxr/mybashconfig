parse_git_branch() {
     git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/(\1)/'
}
#Prompt
#export PS1="\\$\u@\[\e[32m\]\w\[\e[m\]\n\t# "
export PS1="\\$\u@\[\e[32m\]\w\[\e[m\]\n\t \[\e[91m\]\$(parse_git_branch)\[\e[00m\]$ "

